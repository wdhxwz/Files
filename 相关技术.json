            ],
            "success":[
                "./jsvc -jvm server -java-home /usr/local/jdk1.8.0_161 -pidfile /usr/local/tomcat-8.5.29/jsvc.pid -wait 10 -outfile /usr/local/tomcat-8.5.29/logs/catalina.log -errfile /usr/local/tomcat-8.5.29/logs/catalina.err -Djava.util.logging.config.file=/usr/local/tomcat-8.5.29/conf/logging.properties -Djava.util.logging.manager=org.apache.juli.ClassLoaderLogManager -classpath /usr/local/tomcat-8.5.29/bin/bootstrap.jar:/usr/local/tomcat-8.5.29/bin/tomcat-juli.jar:/usr/local/tomcat-8.5.29/bin/commons-daemon.jar -Dcatalina.base=/usr/local/tomcat-8.5.29/ -Dcatalina.home=/usr/local/tomcat-8.5.29 -Djava.io.tmpdir=/usr/local/tomcat-8.5.29/temp org.apache.catalina.startup.Bootstrap ",
